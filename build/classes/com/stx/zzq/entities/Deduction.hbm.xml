<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.stx.zzq.entities">
	<class name="Deduction" table="deduction">
		<id name="deductionId" column="deduction_id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="positionId" type="java.lang.String" column="position_id" />
		<property name="positionName" type="java.lang.String" column="position_name" />
		<property name="departmentId" type="java.lang.String" column="department_id" />
		<property name="departmentName" type="java.lang.String" column="department_name" />
		<property name="trafficWage" column="traffic_wage" type="java.lang.String" />
		<property name="secureReduce" column="secure_reduce" type="java.lang.String" />
		<property name="taxReduce" column="tax_reduce" type="java.lang.String" />
 		<property name="totalReduce" column="total_reduce" type="java.lang.String" />
	</class>
</hibernate-mapping>
 